											"physicalType": "String"
										},
										"sink": {
											"name": "col1",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "col2",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "col2",
											"type": "String"
										}
									},
									{
										"source": {
											"name": "col3",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "col3",
											"type": "String"
										}
									}
								],
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "SourceDataset_2we",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "DestinationDataset_2we",
								"type": "DatasetReference"
							}
						]
