							}
						},
						"inputs": [
							{
								"referenceName": "SourceDataset_2we",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "DestinationDataset_2we",
								"type": "DatasetReference"
							}
						]
					}
				],
				"parameters": {
					"windowStart": {
						"type": "String"
					},
					"windowEnd": {
						"type": "String"
					}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/SourceDataset_2we')]",
				"[concat(variables('factoryId'), '/datasets/DestinationDataset_2we')]"
