						"windowStart": {
							"type": "Expression",
							"value": "@trigger().outputs.windowStartTime"
						},
						"windowEnd": {
							"type": "Expression",
							"value": "@trigger().outputs.windowEndTime"
						}
					}
				},
				"type": "TumblingWindowTrigger",
				"typeProperties": {
					"frequency": "Minute",
					"interval": 5,
					"startTime": "2021-05-05T08:40:00Z",
					"endTime": "2021-05-06T08:50:00Z",
					"delay": "00:00:00",
					"maxConcurrency": 50,
					"retryPolicy": {
						"intervalInSeconds": 30
					},
					"dependsOn": []
				}
			},
			"dependsOn": []
		}
	]
