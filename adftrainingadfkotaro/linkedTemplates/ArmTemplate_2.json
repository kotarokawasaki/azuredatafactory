						},
						"windowEnd": {
							"type": "Expression",
							"value": "[parameters('Trigger_exceltosql_properties_pipeline_parameters_windowEnd')]"
						}
					}
				},
				"type": "TumblingWindowTrigger",
				"typeProperties": {
					"frequency": "Minute",
					"interval": 5,
					"startTime": "2021-05-05T09:50:00Z",
					"endTime": "2021-05-13T10:41:00Z",
					"delay": "00:00:00",
