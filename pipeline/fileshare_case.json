{
	"name": "fileshare_case",
	"properties": {
		"activities": [
			{
				"name": "fileshare_copy",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "AzureFileStorageReadSettings",
							"maxConcurrentConnections": 1,
							"recursive": true,
							"deleteFilesAfterCompletion": false
						},
						"formatSettings": {
							"type": "BinaryReadSettings"
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureFileStorageWriteSettings",
							"maxConcurrentConnections": 1
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "fileshare_dataset1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "fileshare_dataset2",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}